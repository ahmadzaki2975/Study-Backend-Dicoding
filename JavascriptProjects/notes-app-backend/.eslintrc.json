{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "google",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style" : ["error", "windows"],
        "quotes": ["error", "double"],
        "eol-last" : 0,
        "object-curly-spacing": ["error", "always"]
    }
}
